{
  "name": "UnBlocker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Unblock Ads",
  "homepage_url": "http://www.laplanta.com.br",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Unblocker"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}